{
    "designSystemAnalysis": {
      "name": "Modern PWA Beauty App Design System",
      "version": "1.0.0",
      "description": "Design system extracted from modern beauty/hairstyling PWA interface screenshots"
    },
    "colorPalette": {
      "primary": {
        "purple": "#8B7CC8",
        "purpleLight": "#B8A9E8", 
        "purpleDark": "#6B5CA0"
      },
      "secondary": {
        "coral": "#E8A688",
        "coralLight": "#F4C4A8",
        "coralDark": "#D8956E"
      },
      "neutral": {
        "white": "#FFFFFF",
        "lightGray": "#F8F8F8",
        "mediumGray": "#E5E5E5",
        "darkGray": "#666666",
        "black": "#2D2D2D"
      },
      "gradients": {
        "backgroundWarm": "linear-gradient(135deg, #F5E6D3 0%, #E8D5C4 100%)",
        "cardOverlay": "linear-gradient(180deg, rgba(0,0,0,0) 0%, rgba(0,0,0,0.3) 100%)"
      }
    },
    "typography": {
      "fontFamily": {
        "primary": "SF Pro Display, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif"
      },
      "fontSizes": {
        "xs": "12px",
        "sm": "14px", 
        "base": "16px",
        "lg": "18px",
        "xl": "20px",
        "2xl": "24px",
        "3xl": "32px",
        "4xl": "48px"
      },
      "fontWeights": {
        "regular": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700
      },
      "headingStyles": {
        "h1": {
          "fontSize": "4xl",
          "fontWeight": "bold",
          "lineHeight": "1.1",
          "letterSpacing": "-0.02em"
        },
        "h2": {
          "fontSize": "3xl",
          "fontWeight": "bold",
          "lineHeight": "1.2"
        },
        "h3": {
          "fontSize": "2xl",
          "fontWeight": "semibold", 
          "lineHeight": "1.3"
        }
      }
    },
    "layoutPatterns": {
      "mobileFirst": true,
      "containerMaxWidth": "1200px",
      "gridSystems": {
        "mobile": {
          "columns": 1,
          "gap": "16px",
          "padding": "16px"
        },
        "tablet": {
          "columns": 2,
          "gap": "24px", 
          "padding": "24px"
        },
        "desktop": {
          "columns": "auto-fit",
          "gap": "32px",
          "padding": "40px"
        }
      },
      "navigationPatterns": {
        "mobile": {
          "type": "bottom-navigation",
          "height": "80px",
          "itemCount": 4,
          "style": "tab-based"
        },
        "desktop": {
          "type": "top-horizontal",
          "height": "64px",
          "style": "menu-based"
        }
      }
    },
    "componentStyles": {
      "cards": {
        "base": {
          "borderRadius": "16px",
          "padding": "0",
          "shadow": "0 4px 20px rgba(0, 0, 0, 0.08)",
          "background": "white",
          "overflow": "hidden"
        },
        "workshop": {
          "imageAspectRatio": "16:9",
          "contentPadding": "20px",
          "titleFontSize": "lg",
          "titleFontWeight": "semibold",
          "metaFontSize": "sm",
          "metaColor": "darkGray"
        },
        "hairstyle": {
          "imageAspectRatio": "3:4",
          "overlay": "gradient",
          "favoriteButton": {
            "position": "top-right",
            "size": "32px",
            "background": "rgba(255, 255, 255, 0.9)",
            "backdropBlur": "8px"
          }
        },
        "hero": {
          "borderRadius": "20px",
          "minHeight": "200px",
          "textOverlay": true,
          "gradientOverlay": "rgba(0, 0, 0, 0.4)"
        }
      },
      "buttons": {
        "primary": {
          "background": "linear-gradient(135deg, #8B7CC8 0%, #6B5CA0 100%)",
          "color": "white",
          "padding": "14px 28px",
          "borderRadius": "12px",
          "fontSize": "base",
          "fontWeight": "medium",
          "shadow": "0 4px 12px rgba(139, 124, 200, 0.3)",
          "hoverTransform": "translateY(-2px)"
        },
        "secondary": {
          "background": "linear-gradient(135deg, #E8A688 0%, #D8956E 100%)",
          "color": "white", 
          "padding": "14px 28px",
          "borderRadius": "12px",
          "fontSize": "base",
          "fontWeight": "medium"
        },
        "ghost": {
          "background": "rgba(139, 124, 200, 0.1)",
          "color": "#8B7CC8",
          "border": "1px solid rgba(139, 124, 200, 0.2)",
          "padding": "12px 24px",
          "borderRadius": "12px"
        },
        "tab": {
          "background": "transparent",
          "activeBackground": "#8B7CC8",
          "color": "darkGray",
          "activeColor": "white",
          "padding": "8px 20px",
          "borderRadius": "20px",
          "fontSize": "sm",
          "fontWeight": "medium"
        }
      },
      "inputs": {
        "base": {
          "background": "rgba(245, 245, 245, 0.8)",
          "border": "1px solid transparent",
          "focusBorder": "1px solid #8B7CC8",
          "borderRadius": "12px",
          "padding": "14px 16px",
          "fontSize": "base",
          "placeholder": "#999999"
        },
        "search": {
          "borderRadius": "20px",
          "padding": "12px 16px 12px 40px",
          "iconLeft": true,
          "background": "rgba(255, 255, 255, 0.9)",
          "backdropBlur": "8px"
        }
      },
      "navigation": {
        "bottomNav": {
          "background": "rgba(255, 255, 255, 0.95)",
          "backdropBlur": "20px",
          "border": "1px solid rgba(0, 0, 0, 0.05)",
          "itemSpacing": "equal",
          "iconSize": "24px",
          "activeColor": "#8B7CC8",
          "inactiveColor": "#999999",
          "labelFontSize": "xs"
        },
        "topNav": {
          "background": "rgba(255, 255, 255, 0.95)",
          "backdropBlur": "20px",
          "height": "64px",
          "padding": "0 24px",
          "itemSpacing": "32px"
        }
      },
      "lists": {
        "workshop": {
          "itemPadding": "16px",
          "imageSize": "80px",
          "imageRadius": "12px",
          "spacing": "12px",
          "titleFontSize": "lg",
          "metaFontSize": "sm",
          "metaColor": "darkGray"
        },
        "vertical": {
          "itemSpacing": "12px",
          "padding": "16px",
          "separatorColor": "rgba(0, 0, 0, 0.05)"
        }
      }
    },
    "imageStyles": {
      "aspectRatios": {
        "square": "1:1",
        "portrait": "3:4",
        "landscape": "16:9",
        "wide": "21:9"
      },
      "borderRadius": {
        "small": "8px",
        "medium": "12px", 
        "large": "16px",
        "xlarge": "20px"
      },
      "overlays": {
        "gradient": "linear-gradient(180deg, rgba(0,0,0,0) 0%, rgba(0,0,0,0.6) 100%)",
        "solid": "rgba(0, 0, 0, 0.4)",
        "blur": "backdrop-blur(8px)"
      }
    },
    "spacing": {
      "xs": "4px",
      "sm": "8px", 
      "md": "12px",
      "lg": "16px",
      "xl": "20px",
      "2xl": "24px",
      "3xl": "32px",
      "4xl": "40px",
      "5xl": "48px",
      "6xl": "64px"
    },
    "animations": {
      "transitions": {
        "default": "all 0.2s cubic-bezier(0.4, 0, 0.2, 1)",
        "slow": "all 0.3s cubic-bezier(0.4, 0, 0.2, 1)",
        "bounce": "all 0.3s cubic-bezier(0.68, -0.55, 0.265, 1.55)"
      },
      "hover": {
        "scale": "transform: scale(1.02)",
        "lift": "transform: translateY(-4px)",
        "shadowIncrease": "box-shadow: 0 8px 32px rgba(0, 0, 0, 0.12)"
      },
      "loading": {
        "skeleton": "background: linear-gradient(90deg, #f0f0f0 25%, #e0e0e0 50%, #f0f0f0 75%)",
        "pulse": "opacity: 0.6"
      }
    },
    "shadows": {
      "small": "0 2px 8px rgba(0, 0, 0, 0.06)",
      "medium": "0 4px 20px rgba(0, 0, 0, 0.08)",
      "large": "0 8px 32px rgba(0, 0, 0, 0.12)",
      "colored": "0 4px 20px rgba(139, 124, 200, 0.15)"
    },
    "visualEffects": {
      "glassmorphism": {
        "background": "rgba(255, 255, 255, 0.1)",
        "backdropFilter": "blur(20px)",
        "border": "1px solid rgba(255, 255, 255, 0.2)"
      },
      "gradientBorders": {
        "primary": "linear-gradient(135deg, #8B7CC8, #E8A688)",
        "subtle": "linear-gradient(135deg, rgba(139, 124, 200, 0.3), rgba(232, 166, 136, 0.3))"
      }
    },
    "breakpoints": {
      "mobile": "320px",
      "tablet": "768px",
      "desktop": "1024px",
      "wide": "1400px"
    },
    "designPrinciples": {
      "visual": [
        "Warm, inviting color palette with purple and coral accents",
        "Generous use of white space and clean layouts",
        "Soft, rounded corners throughout (12px-20px radius)",
        "High-quality, aspirational imagery as focal points",
        "Subtle gradients and soft shadows for depth",
        "Glass morphism effects for modern feel"
      ],
      "layout": [
        "Card-based information architecture",
        "Mobile-first responsive design",
        "Clear visual hierarchy with typography scale",
        "Grid-based layouts with consistent spacing",
        "Bottom navigation for mobile, top for desktop",
        "Generous padding and comfortable touch targets"
      ],
      "interaction": [
        "Subtle hover states with scale and lift effects",
        "Clear call-to-action buttons with gradients",
        "Tab-based navigation with active states",
        "Immediate visual feedback for actions",
        "Smooth transitions and micro-animations",
        "Accessible color contrast and focus states"
      ],
      "content": [
        "Image-heavy presentation with overlay text",
        "Category-based content organization",
        "Preview-focused design (thumbnails, short descriptions)",
        "Social proof elements (member counts, etc.)",
        "Progress indicators and status updates",
        "Personalized welcome messages"
      ]
    },
    "contentStructure": {
      "cardLayouts": {
        "imageCard": {
          "imagePosition": "top",
          "imageHeight": "60%",
          "contentPadding": "16px",
          "titleMaxLines": 2,
          "metaMaxLines": 1
        },
        "horizontalCard": {
          "imageWidth": "80px",
          "imagePosition": "left", 
          "contentPadding": "12px",
          "alignment": "flex-start"
        },
        "heroCard": {
          "textOverlay": true,
          "overlayPosition": "bottom-left",
          "overlayPadding": "24px",
          "textColor": "white"
        }
      },
      "listLayouts": {
        "workshop": {
          "imageSize": "80x80",
          "titleFontSize": "18px",
          "metaFontSize": "14px",
          "spacing": "16px",
          "actionButton": "right"
        },
        "gallery": {
          "columns": "2 mobile, 3+ desktop",
          "aspectRatio": "3:4",
          "gap": "8px",
          "hoverOverlay": true
        }
      }
    },
    "stateManagement": {
      "loading": {
        "skeleton": "animated gradient blocks",
        "spinner": "circular with brand color",
        "progressive": "content loads in stages"
      },
      "empty": {
        "illustration": "subtle, brand-aligned",
        "message": "encouraging, actionable",
        "cta": "prominent button"
      },
      "error": {
        "color": "#E74C3C",
        "background": "rgba(231, 76, 60, 0.1)",
        "border": "1px solid rgba(231, 76, 60, 0.2)"
      },
      "success": {
        "color": "#27AE60",
        "background": "rgba(39, 174, 96, 0.1)",
        "border": "1px solid rgba(39, 174, 96, 0.2)"
      }
    },
    "implementationNotes": {
      "pwaRequirements": [
        "Service worker for offline functionality",
        "App manifest with proper icons",
        "Responsive design across all breakpoints",
        "Touch-friendly interface elements",
        "Fast loading with skeleton screens",
        "Cached images for offline viewing"
      ],
      "performanceConsiderations": [
        "Lazy loading for images",
        "Progressive image loading",
        "Optimized bundle sizes",
        "CSS-in-JS or utility classes",
        "Efficient state management",
        "Minimal re-renders"
      ],
      "accessibilityFeatures": [
        "WCAG 2.1 AA compliance",
        "Proper heading hierarchy",
        "Alt text for images",
        "Keyboard navigation",
        "Focus indicators",
        "Screen reader support"
      ]
    }
  }